  "bugs": {
    "url": "https://github.com/MateuszJajkowicz/CSPro/issues"
  },
  "homepage": "https://github.com/MateuszJajkowicz/CSPro#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^8.1.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^7.2.2",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "@types/express": "^4.17.17"
  }
